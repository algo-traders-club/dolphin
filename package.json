    "bs58": "^5.0.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "hono": "^3.11.7"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3"
