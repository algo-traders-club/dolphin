# Solana RPC Endpoint (Devnet)
SOLANA_RPC_URL=https://api.devnet.solana.com

# Agent wallet private key (base58 encoded)
# WARNING: This is not secure for production use
# NEVER commit the actual .env file with real keys to version control
WALLET_PRIVATE_KEY=

# Orca Whirlpool addresses (Devnet)
WHIRLPOOL_PROGRAM_ID=whirLbMiicVdio4qvUfM5KAg6Ct8VwpYzGff3uctyCc
USDC_SOL_WHIRLPOOL_ADDRESS=

# Token mints (Devnet)
SOL_MINT=So11111111111111111111111111111111111111112
USDC_MINT=

# Liquidity position parameters
DEFAULT_POSITION_LOWER_PRICE=0.01
DEFAULT_POSITION_UPPER_PRICE=0.05
DEFAULT_LIQUIDITY_AMOUNT_USDC=10

# Server configuration
PORT=3000
HOST=0.0.0.0

# Database configuration
DATABASE_URL=postgres://postgres:postgres@timescaledb:5432/cashflow

# Auto-rebalancing configuration
# Enable auto-rebalancing (true/false)
REBALANCE_ENABLED=false
# Percentage threshold for rebalancing (default: 5)
REBALANCE_THRESHOLD_PERCENT=5
# Minimum time between rebalances in minutes (default: 60)
MIN_REBALANCE_INTERVAL_MINUTES=60
# Position width percentage (default: 20)
POSITION_WIDTH_PERCENT=20
# Maximum number of rebalances per day (default: 6)
MAX_DAILY_REBALANCES=6

# Logging configuration
# Available log levels: debug, info, warn, error
LOG_LEVEL=info
# Enable file logging (true/false)
ENABLE_FILE_LOGGING=false
# Log directory (relative to project root)
# LOG_DIRECTORY=logs
# Log filename
# LOG_FILENAME=cashflow.log
